doctype html
html(lang="en")
	head
		include /includes/head
		script(src="js/robin/dashboard.js")
	body
		div.container
			include /includes/header

			div.row.clearfix
				div.col-md-2.column
					include /includes/leftcolumn
				div.col-md-6.column
					label(for="basiccmd") Basic command
					input#basiccmd(type="text" placeholder="Basic command")
					button#basiccmdSubmit Send
					br
					label(for="extendedcmd") Extended command
					input#extendedcmd(type="text" placeholder="Extended command")
					button#extendedcmdSubmit Send
				div.col-md-4.column
					include /includes/rightcolumn
